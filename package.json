{
  "name": "choreograph-parent",
  "version": "1.0.0",
  "description": "An OSS tool to orchestrate docker based workers. Comes with a web-UI and an API.",
  "main": "",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node server/buildHTML.js",
    "build": "npm-run-all clean-dist test lint:build build:html && babel-node server/build.js",
    "build:alias": "babel-node server/build.js",
    "start": "node_modules/.bin/babel-node server/start.js",
    "open:src": "babel-node server/srcServer.js",
    "lint": "esw webpack.config.* src server",
    "lint:build": "node_modules/.bin/esw webpack.config.* src server -c ./.eslintrc-build",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint src server webpack.config.* --fix",
    "test": "mocha --reporter spec server/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "dev": "node_modules/.bin/forever start -c node_modules/.bin/babel-node server/startMessage.js && npm-run-all --parallel open:src lint:watch test:watch"
  },
  "author": "Ashish Chaudhary",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "byline": "^5.0.0",
    "dockerode": "^2.3.0",
    "express": "^4.14.0",
    "forever": "^0.15.2",
    "jquery": "^3.1.0",
    "material-ui": "^0.15.2",
    "mkdirp": "^0.5.1",
    "os-usage": "git+https://github.com/ningt/os-usage.git",
    "os-utils": "0.0.14",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redis-scheduler": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "semantic-ui-css": "^2.2.1",
    "simplewebrtc": "^2.2.0",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6",
    "sqlite3": "^3.1.4",
    "superagent": "^2.2.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "compression": "^1.6.2",
    "eslint": "^3.0.1",
    "cross-env": "2.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-cli": "^6.10.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "colors": "^1.1.2",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^5.1.1",
    "eslint-watch": "^2.1.12",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.9.0",
    "jsdom": "^9.3.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "npm-run-all": "^2.3.0",
    "open": "^7.0.0",
    "react-addons-test-utils": "^15.2.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.1.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  },
  "keywords": [
    "react",
    "reactjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tocttou/choreograph-parent"
  }
}
